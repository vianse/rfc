<p id="notice"><%= notice %></p>

<h1>Listing Myrfcs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Rfc</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Cp</th>
      <th>Comercio</th>
      <th>Referencia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myrfcs.each do |myrfc| %>
      <tr>
        <td><%= myrfc.name %></td>
        <td><%= myrfc.rfc %></td>
        <td><%= myrfc.address %></td>
        <td><%= myrfc.phone %></td>
        <td><%= myrfc.email %></td>
        <td><%= myrfc.cp %></td>
        <td><%= myrfc.comercio_id %></td>
        <td><%= myrfc.referencia %></td>
        <td><%= link_to 'Show', myrfc %></td>
        <td><%= link_to 'Edit', edit_myrfc_path(myrfc) %></td>
        <td><%= link_to 'Destroy', myrfc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myrfc', new_myrfc_path %>
